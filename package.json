{
  "name": "cardano-asterium-explorer",
  "description": "The cardano community explorer",
  "version": "1.0.0",
  "private": true,
  "author": "Mart√≠n Schere Salamanco | Asterium",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
