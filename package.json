    "moment": "2.29.3",
    "next": "12.1.5",
    "nextjs-progressbar": "0.0.14",
    "prisma": "3.13.0",
    "react": "^17.1.0",
    "react-debounce-input": "3.2.5",
    "react-dom": "^17.1.0",
    "react-feather": "2.0.9",
