    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/inter": "4.5.9",
    "@prisma/client": "3.13.0",
    "@types/lunr": "2.3.4",
    "@types/react-window": "1.8.5",
    "ahooks": "3.3.10",
